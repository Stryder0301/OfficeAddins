<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>1d6c7edb-b230-4fcb-9ab9-6aabbf06f298</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Highstreet</ProviderName>  
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Highstreet Insert"/> 
  <Description DefaultValue="Quick access to Highstreet branding materials."/> 
  <IconUrl DefaultValue="https://www.contoso.com/assets/HighstreetLogo-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://www.contoso.com/assets/LogomarkBlue.png"/> <!-- CHANGE TO DISTRIBUTION URL -->
  <SupportUrl DefaultValue="https://www.contoso.com/help"/> <!-- CHANGE TO DISTRIBUTION URL -->
  <AppDomains>
    <AppDomain>https://www.contoso.com</AppDomain> <!-- CHANGE TO DISTRIBUTION URL -->
  </AppDomains>
  <Hosts>
    <Host Name="Presentation"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://www.contoso.com/taskpane.html"/> <!-- CHANGE TO DISTRIBUTION URL -->
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Presentation">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>

          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://www.contoso.com/assets/HighstreetLogo-32.png"/> <!-- CHANGE TO DISTRIBUTION URL -->
        <bt:Image id="Icon.32x32" DefaultValue="https://www.contoso.com/assets/HighstreetLogo-32.png"/> <!-- CHANGE TO DISTRIBUTION URL -->
        <bt:Image id="IconBlack.32x32" DefaultValue="https://www.contoso.com/assets/HighstreetLogoBlack-32.png"/> <!-- CHANGE TO DISTRIBUTION URL -->
        <bt:Image id="Icon.80x80" DefaultValue="https://www.contoso.com/assets/HighstreetLogo-80.png"/> <!-- CHANGE TO DISTRIBUTION URL -->
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="https://www.contoso.com/commands.html"/> <!-- CHANGE TO DISTRIBUTION URL -->
        <bt:Url id="Taskpane.Url" DefaultValue="https://www.contoso.com/taskpane.html"/> <!-- CHANGE TO DISTRIBUTION URL -->
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with your sample add-in!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Commands Group"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Highstreet Insert"/>
        <bt:String id="ContextMenuButton.Label" DefaultValue="Highstreet Insert"/>

        <bt:String id="NeXtechLab.Label" DefaultValue="NeXtech Lab™"/>
        <bt:String id="NeXtech.Label" DefaultValue="NeXtech™"/>
        <bt:String id="DeliveryX.Label" DefaultValue="DeliveryX™"/>
        <bt:String id="UpNEXT.Label" DefaultValue="UpNEXT™"/>
        <bt:String id="DataRefleXtion.Label" DefaultValue="Data RefleXtion™"/>
        <bt:String id="BridgeXing.Label" DefaultValue="BridgeXing™"/>
        <bt:String id="FastraX.Label" DefaultValue="FastraX™"/>
        <bt:String id="RouteX.Label" DefaultValue="RouteX™"/>
        <bt:String id="ProcessDynamiX.Label" DefaultValue="Process-DynamiX™"/>
        <bt:String id="Bridges.Label" DefaultValue="Bridges™"/>
        <bt:String id="PaySynX.Label" DefaultValue="Pay-SynX™"/>

        <bt:String id="HighstreetLogoPrimary.Label" DefaultValue="Primary Highstreet Logo"/>
        <bt:String id="HighstreetLogoWhite.Label" DefaultValue="White Highstreet Logo"/>
        <bt:String id="HighstreetLogoBlack.Label" DefaultValue="Black Highstreet Logo"/>
        <bt:String id="BridgesLogo.Label" DefaultValue="Bridges Logo"/>
        <bt:String id="LogomarkBlue.Label" DefaultValue="Blue Logomark"/>
        <bt:String id="LogomarkBlack.Label" DefaultValue="Black Logomark"/>
        <bt:String id="LogomarkWhite.Label" DefaultValue="White Logomark"/>

      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your sample add-in loaded successfully. Go to the HOME tab and click the 'Show Task Pane' button to get started."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to Show Highstreet Insert Taskpane"/>
        <bt:String id="ContextMenuButton.Tooltip" DefaultValue="Highstreet Insert"/>

        <bt:String id="NeXtechLab.Tooltip" DefaultValue="Insert NeXtech Lab™"/>
        <bt:String id="NeXtech.Tooltip" DefaultValue="Insert NeXtech™"/>
        <bt:String id="DeliveryX.Tooltip" DefaultValue="Insert DeliveryX™"/>
        <bt:String id="UpNEXT.Tooltip" DefaultValue="Insert UpNEXT™"/>
        <bt:String id="DataRefleXtion.Tooltip" DefaultValue="Insert Data RefleXtion™"/>
        <bt:String id="BridgeXing.Tooltip" DefaultValue="Insert BridgeXing™"/>
        <bt:String id="FastraX.Tooltip" DefaultValue="Insert FastraX™"/>
        <bt:String id="RouteX.Tooltip" DefaultValue="Insert RouteX™"/>
        <bt:String id="ProcessDynamiX.Tooltip" DefaultValue="Insert Process-DynamiX™"/>
        <bt:String id="Bridges.Tooltip" DefaultValue="Insert Bridges™"/>
        <bt:String id="PaySynX.Tooltip" DefaultValue="Insert Pay-SynX™"/>

        <bt:String id="HighstreetLogoPrimary.Tooltip" DefaultValue="Insert Primary Highstreet Logo"/>
        <bt:String id="HighstreetLogoWhite.Tooltip" DefaultValue="Insert White Highstreet Logo"/>
        <bt:String id="HighstreetLogoBlack.Tooltip" DefaultValue="Insert Black Highstreet Logo"/>
        <bt:String id="BridgesLogo.Tooltip" DefaultValue="Insert Bridges Logo"/>
        <bt:String id="LogomarkBlue.Tooltip" DefaultValue="Insert Blue Logomark"/>
        <bt:String id="LogomarkWhite.Tooltip" DefaultValue="Insert White Logomark"/>
        <bt:String id="LogomarkBlack.Tooltip" DefaultValue="Insert Black Logomark"/>

      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>